@model IEnumerable<string>

@Html.ActionLink("Home", "List", "StoreProduct", null,
    new { @class = "btn btn-block btn-default btn-lg" })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        controller = "StoreProduct",
        action = "List",
        category = link,
        page = 1
    }, new
    {
        @class = "btn btn-block btn-default btn-md " + 
        (link == ViewBag.SelectedCategory ? "btn-primary selected" : "")
    })
}